<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomTextView">
        <attr name="fontName" format="string" />
    </declare-styleable>

    <declare-styleable name="Lock9View">
        <attr name="lock9_nodeSrc" format="color|reference" />
        <attr name="lock9_nodeOnSrc" format="color|reference" />
        <attr name="lock9_nodeSize" format="dimension" />
        <attr name="lock9_nodeAreaExpand" format="dimension" />
        <attr name="lock9_nodeOnAnim" format="reference" />
        <attr name="lock9_lineColor" format="color" />
        <attr name="lock9_lineWidth" format="dimension" />
        <attr name="lock9_padding" format="dimension" />
        <attr name="lock9_spacing" format="dimension" />
        <attr name="lock9_autoLink" format="boolean" />
        <attr name="lock9_enableVibrate" format="boolean" />
        <attr name="lock9_vibrateTime" format="integer" />
    </declare-styleable>

    <declare-styleable name="PlaybackControlView">
        <attr name="show_timeout" />
        <attr name="rewind_increment" />
        <attr name="fastforward_increment" />
    </declare-styleable>
    <declare-styleable name="SettingBasic">
        <attr name="settingIcon" format="string" />
        <attr name="settingTitle" format="reference" />
        <attr name="settingCaption" format="reference" />
        <attr name="settingMinApi" format="integer" />
    </declare-styleable>
    <declare-styleable name="SettingWithSwitchView">
        <attr name="settingIcon" />
        <attr name="settingTitle" />
        <attr name="settingCaption" />
        <attr name="settingMinApi" />
        <attr name="settingDefaultValue" format="boolean" />
        <attr name="settingPreferenceKey" format="reference" />
    </declare-styleable>

    <declare-styleable name="NavigationEntry">
        <attr name="itemText" format="reference" />
        <attr name="itemIcon" format="string" />
    </declare-styleable>

    <declare-styleable name="AboutCreator">
        <attr name="headerImage" format="reference" />
        <attr name="displayImage" format="reference" />
        <attr name="name" format="string" />
        <attr name="role" format="string" />
        <attr name="description" format="string" />
    </declare-styleable>

    <declare-styleable name="AboutLink">
        <attr name="linkIcon" format="string" />
        <attr name="linkTitle" format="string" />
        <attr name="linkDescription" format="string" />
    </declare-styleable>

    <declare-styleable name="ThemedFab">
        <attr name="liz_icon" format="string" />
    </declare-styleable>

    <declare-styleable name="ThemedTextView">
        <attr name="liz_color_style" format="enum">
            <enum name="accent" value="1" />
            <enum name="primary" value="2" />
            <enum name="text" value="3" />
            <enum name="sub_text" value="4" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ThemedLinearLayout">
        <attr name="liz_background_style" format="enum">
            <enum name="normal" value="1" />
            <enum name="card" value="2" />
            <enum name="primary" value="3" />
            <enum name="accent" value="4" />
        </attr>
    </declare-styleable>


    <declare-styleable name="CustomSeekBar">
        <attr name="minValue" format="integer" />
        <attr name="maxValue" format="integer" />
        <attr name="progress" format="integer" />
        <attr name="thumbDrawable" format="reference"></attr>
        <attr name="thumbPressedDrawable" format="reference"></attr>
        <attr name="defaultBackgroundColor" format="color"></attr>
        <attr name="defaultBackgroundRangeColor" format="color"></attr>
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>

    <declare-styleable name="StickerView">
        <attr name="showIcons" format="boolean" />
        <attr name="showBorder" format="boolean" />
        <attr name="bringToFrontCurrentSticker" format="boolean" />
        <attr name="borderColor" format="color" />
        <attr name="borderAlpha" format="integer" />
    </declare-styleable>

    <declare-styleable name="PhotoEditorView">
        <!-- {@deprecated Use photo_src instead.} -->
        <attr name="photo_src" format="integer" />
    </declare-styleable>

    <declare-styleable name="scv_CropImageView">
        <attr name="scv_img_src" format="reference" />
        <attr name="scv_crop_mode">
            <enum name="fit_image" value="0" />
            <enum name="ratio_4_3" value="1" />
            <enum name="ratio_3_4" value="2" />
            <enum name="square" value="3" />
            <enum name="ratio_16_9" value="4" />
            <enum name="ratio_9_16" value="5" />
            <enum name="free" value="6" />
            <enum name="custom" value="7" />
            <enum name="circle" value="8" />
            <enum name="circle_square" value="9" />
        </attr>
        <attr name="scv_background_color" format="reference|color" />
        <attr name="scv_overlay_color" format="reference|color" />
        <attr name="scv_frame_color" format="reference|color" />
        <attr name="scv_handle_color" format="reference|color" />
        <attr name="scv_guide_color" format="reference|color" />
        <attr name="scv_guide_show_mode">
            <enum name="show_always" value="1" />
            <enum name="show_on_touch" value="2" />
            <enum name="not_show" value="3" />
        </attr>
        <attr name="scv_handle_show_mode">
            <enum name="show_always" value="1" />
            <enum name="show_on_touch" value="2" />
            <enum name="not_show" value="3" />
        </attr>
        <attr name="scv_handle_size" format="dimension" />
        <attr name="scv_touch_padding" format="dimension" />
        <attr name="scv_min_frame_size" format="dimension" />
        <attr name="scv_frame_stroke_weight" format="dimension" />
        <attr name="scv_guide_stroke_weight" format="dimension" />
        <attr name="scv_crop_enabled" format="boolean" />
        <attr name="scv_initial_frame_scale" format="float" />
        <attr name="scv_animation_enabled" format="boolean" />
        <attr name="scv_animation_duration" format="integer" />
        <attr name="scv_handle_shadow_enabled" format="boolean" />
    </declare-styleable>


</resources>